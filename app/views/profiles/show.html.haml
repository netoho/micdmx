- model_class = Profile1
.page-header
  %h1=t '.title', :default => model_class.model_name.human.titleize

%p
  %strong= model_class.human_attribute_name(:profile) + ':'
  %br
  = @profile1.profile


= link_to t('.back', :default => t("helpers.links.back")), profile1s_path, :class => 'btn btn-default'
= link_to t('.edit', :default => t("helpers.links.edit")), edit_profile1_path(@profile1), :class => 'btn btn-default'
= link_to t('.destroy', :default => t("helpers.links.destroy")), profile1_path(@profile1), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
